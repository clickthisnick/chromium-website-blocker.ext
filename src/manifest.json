{
  "name": "WebsiteBlocker",
  "description": "Developer Friendly Website Blocker",
  "background": {
    "page": "popup.html"
  },
  "permissions": [
    "tabs",
    "*://*/*",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  }
}
